<script>
    var imagenes = ["https://via.placeholder.com/150x200", "https://via.placeholder.com/150x150", "https://via.placeholder.com/220x150", "https://via.placeholder.com/300x150", "https://via.placeholder.com/500x150"];

    var i = Math.floor(Math.random()*imagenes.length);
    console.log(i)

    document.getElementById("myImg").setAttribute("src", imagenes[i]);
</script>
